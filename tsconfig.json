{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["DOM", "ES2020"],
      "module": "ESNext",
      "jsx": "react-jsx",
      "moduleResolution": "Bundler",
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true
    },
    "include": ["src"]
  }
  